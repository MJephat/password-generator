import random

uppercase_character = "ABCDEFGHIJKLMNOPQRSTUVWXYZ"
lowercase_character = uppercase_character.lower()
digits = "0123456789"
symbols = "*&^%$#@!"

upper, lower, numbers, symb = True, True, True, True

all = ""

if uppercase_character:
    all += upper
if lowercase_character:
    all += lower
if digits:
    all += numbers
if symbols:
    all += symb

length = 13
amount = 10

for x in range(amount)
    password = "".join(random.sample(length,all))
    print(password)